{
  "name": "language-mkq",
  "displayName": "Language MKQ",
  "description": "Syntax HighLighting for Language MKQ",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mkq",
      "aliases": ["Mkq", "mkq"],
      "extensions": [".myo", ".mkq"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mkq",
      "scopeName": "source.mkq",
      "path": "./syntaxes/mkq.tmLanguage.json"
    }],
    "snippets": [{
      "language": "mkq",
      "path": "./snippets/snippets.json"
    }]
  }
}
